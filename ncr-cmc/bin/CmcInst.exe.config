<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <!-- Some systems may not have .Net 2.0 framework, only 4.0, or vice versa, but 4.0 is preferred platform. -->
    <supportedRuntime version="v4.0"/>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
  <runtime>
    <!-- Disable Authenticode Signature Publisher Evidence creation (may delay start, requires Internet access) -->
    <generatePublisherEvidence enabled="false"/>
  </runtime>
</configuration>